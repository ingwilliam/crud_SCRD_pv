--Tabla categoriajurado donde se guarda informacion de los select
-- Drop table

-- DROP TABLE public.categoriajurado;

CREATE TABLE public.categoriajurado (
	id int4 NOT NULL, -- Identificador de la tabla, numérico consecutivo, dado manualmente
	descripcion varchar(100) NULL, -- Descripción del valor del parámetro
	estado varchar(3) NULL, -- Estado del parámetro, A = Activo, I = Inactivo
	nombre varchar(100) NULL, -- Nombre o valor del parámetro
	nodo int4 NULL, -- Asocia las categorias de jurados en grupos
	"version" int8 NULL, -- Versión del registro
	actualizado_por varchar(255) NULL, -- Usuario que actualizó el registro
	creado_por varchar(255) NULL, -- Usuario que creó el registro
	fecha_actualizacion timestamp NULL, -- Fecha de actualización
	fecha_creacion timestamp NULL, -- Fecha de creación
	tipo varchar(100) NULL,
	active bool NOT NULL DEFAULT false,
	CONSTRAINT categoria_jurado_pkey PRIMARY KEY (id)
);
COMMENT ON TABLE public.categoriajurado IS 'Parámetro de categoria_jurado';

-- Column comments

COMMENT ON COLUMN public.categoriajurado.id IS 'Identificador de la tabla, numérico consecutivo, dado manualmente';
COMMENT ON COLUMN public.categoriajurado.descripcion IS 'Descripción del valor del parámetro';
COMMENT ON COLUMN public.categoriajurado.estado IS 'Estado del parámetro, A = Activo, I = Inactivo';
COMMENT ON COLUMN public.categoriajurado.nombre IS 'Nombre o valor del parámetro';
COMMENT ON COLUMN public.categoriajurado.nodo IS 'Asocia las categorias de jurados en grupos';
COMMENT ON COLUMN public.categoriajurado."version" IS 'Versión del registro';
COMMENT ON COLUMN public.categoriajurado.actualizado_por IS 'Usuario que actualizó el registro';
COMMENT ON COLUMN public.categoriajurado.creado_por IS 'Usuario que creó el registro';
COMMENT ON COLUMN public.categoriajurado.fecha_actualizacion IS 'Fecha de actualización';
COMMENT ON COLUMN public.categoriajurado.fecha_creacion IS 'Fecha de creación';

